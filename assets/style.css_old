
@font-face {
	src: url('./fonts/gBlank.woff2') format('woff2');
	font-family: 'blank';
}



* {
	font-size: inherit;
	font-variation-settings: inherit;
	margin: 0;
	padding: 0;
	line-height: inherit;
	text-align: inherit;
	font-family: inherit;
	font-weight: inherit;
	font-style: inherit;
	font-kerning: normal;
	color: inherit;
	list-style: none;
	box-sizing: border-box;
}
body::before { /* BREAKPOINT INDICATOR */
	content: 'XXS';
	font-family: Verdana, sans-serif;
	font-size: 9px;
	color: rgba(0,0,0, 0.5);
	position: fixed;
	top: 0px;
	left: 0px;
	display: none;
	z-index: 10000;
}

html.debug * {outline: 1px dashed teal;}
html.debug body:before {display: block;}


